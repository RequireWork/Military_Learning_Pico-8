num_lines = 5

function _draw()
 cls()
 for i = 1, num_lines do
  print("test"..i, 40, 20 + (i * 10), i + 7)
 end
end

------------------------------------------------------
Moving Text Test!

x_pos = 0
y_pos = 100
speed = 1

function _update()
 x_pos += speed
 y_pos -= speed
end

function _draw()
 cls()
 print("moving!", x_pos, y_pos, 7)
end
